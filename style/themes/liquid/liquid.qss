}

/* make use of negative margins for overlapping tabs */
QTabBar::tab:top:selected
{
    border-width: 1;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #fff, stop:1 #fff);
    border: 1px solid #c3bfbf;
    border: 1px solid #7d8389;
    border-bottom: 0px solid #c3bfbf;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #fff, stop:1 #eee);
     background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #e3e3eC, stop:1 #cAcFd5);
    border-right : 0;
}
