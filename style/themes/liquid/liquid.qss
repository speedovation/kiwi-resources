}

/* make use of negative margins for overlapping tabs */
QTabBar::tab:bottom:selected
{
    border-width: 2;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #efefef, stop:1 #dee0e2);
    border: 1px solid #c3bfbf;
    border-top-color: #efefef;/* same as pane color */
}

