QComboBox
{
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #f7f8f9, stop:0.0680112 #f7f8f9, stop:0.0690112 #f1f3f4, stop:0.514566 #e4e7ea, stop:0.52577 #dde1e5, stop:1 #d0d4da);
    border-radius: 1px;
    border: 1px solid #aAaFb5;
    padding: 2px 0;
/*    height: 20px;*/
/*    margin-top: 2px;*/
    
    background-color: #eaeaee;
}

QComboBox[editable="false"]
{
    padding-left: 3px;
    padding-right: 20px;/* space for the arrow */
}

QComboBox QAbstractItemView
{
    border: 0px solid darkgray;
    selection-background-color: qlineargradient(spread:pad, x1:0.453399, y1:0, x2:0.462, y2:1, stop:0.0936281 #5b9bd1, stop:1 #406db8);
    
}

QComboBox::drop-down
{
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 18px;
    border-left-style: solid;
    border-left-color: darkgray;
    border-left-width: 0px;
}

QComboBox[editable="false"]::down-arrow
{
    subcontrol-origin: border;
    subcontrol-position: top right;
    width: 18px;
    position: absolute;
    top: 8px;
    bottom: 2px;
    right: 2px;
}

/* The combobox arrow is on when the popup is open. */
QComboBox[editable="false"]::down-arrow:on
{
    position: relative;
    top: 1px;
    left: 2px;
}

/* Customize editable comboboxes. */
QComboBox[editable="true"]
{
    padding-right: 0px;
    background:#fff;
}

QComboBox[editable="true"]::drop-down
{
    subcontrol-origin: border;
    subcontrol-position: top right;
    width: 32px;
    position: absolute;
    top: 2px;
    bottom: 2px;
    right: 2px;
    
     border-left-style: solid;
    border-left-color: darkgray;
    border-left-width: 1px;
    
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #f7f8f9, stop:0.0680112 #f7f8f9, stop:0.0690112 #f1f3f4, stop:0.514566 #e4e7ea, stop:0.52577 #dde1e5, stop:1 #d0d4da);
/*    border: 1px solid #333;*/
    
}

QComboBox[editable="true"]::drop-down, QComboBox[editable="true"]::drop-down:hover, QComboBox[editable="true"]::drop-down:on
{
    border-width: 0px;
    border-left-width: 0px;/* we need only left and center part */
}

/* Shift the arrow when it's open. */
QComboBox[editable="true"]::down-arrow:on
{
    position: relative;
    top: 1px;
    left: 1px;
}


