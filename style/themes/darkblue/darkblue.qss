QTabWidget
{
    border: none;
}

QTabWidget::pane
{/* The tab widget frame */
    border: none;
    border-top: 1px solid #424b59;
}

QTabBar::tab:top
{
    min-width: 25px;
    padding: 6px 15px;
    margin-right: 1px;
    margin-left: 1px;
    border-top-right-radius: 5px;
    border-top-left-radius: 5px;
}

QTabBar::tab:top:!selected
{
    border-width: 1;
    background: qlineargradient(spread:pad, x1:0.453399, y1:0, x2:0.462, y2:1, stop:0.0936281 #495362, stop:1 #373e4a);
    border: 1px solid #252a31;
    border-bottom-color: transparent;
    margin-top: 1px;
    color: #fff;
}

/* make use of negative margins for overlapping tabs */
QTabBar::tab:top:selected
{
    font-weight: bold;
    border-width: 1;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #f9fafa, stop:1 rgba(255, 255, 255, 255));
    background: qlineargradient(spread:pad, x1:0.453399, y1:0, x2:0.462, y2:1, stop:0.0936281 #E97117, stop:1 #FF6102);
    background: qlineargradient(spread:pad, x1:0.453399, y1:0, x2:0.462, y2:1, stop:0.0936281 #5b9bd1, stop:1 #406db8);/*background : qlineargradient(spread:pad, x1:0.453399, y1:0, x2:0.462, y2:1, stop:0.0936281 #F7EFD8, stop:1 #F7EFD8);*/
    border: 1px solid #1f2328;
    border-bottom: 0px solid #c3bfbf;
}

QTabBar::tab:top:!selected:hover
{
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #5d6879, stop:1 #464f5b);
}

QTabBar::tab:top:selected:hover
{/* background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #fff, stop:1 #f9f8f8);
   background: transparent;*/
}

QTabBar::tab:bottom
{
    min-width: 22.1ex;
    padding: 4px;
    margin-top: 0px;
    margin-right: 8px;
    margin-bottom: 3px;
    margin-left: 0.1px;
    border-bottom-right-radius: 3px;
    border-bottom-left-radius: 3px;
}

QTabBar::tab:bottom:!selected
{
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #edeff1, stop:1 #e3e6e9);
    border: 1px solid #8b939c;/* same as pane color */
}

/* make use of negative margins for overlapping tabs */
QTabBar::tab:bottom:selected
{/* expand/overlap to the left and right by 4px
    margin-left: -0.5px;
    margin-right: -0.5px;*/
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #fff, stop:1 #efefef);
    border: 1px solid #fff;
    border-top-color: #fff;/* same as pane color */
}

QTabBar::tab:bottom:hover
{/*    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #fbfdfe, stop:1 #edeff1);*/
}

QTabBar::tab:bottom:selected:hover
{
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #fff, stop:1 #dee0e2);
}

