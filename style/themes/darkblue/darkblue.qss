    padding: 4px;
    margin-right: 0.5px;
    margin-left: 0.5px;
}

QTabBar::tab:right:!selected
{
    border-width: 3;
    background: qlineargradient(spread:pad, x1:0.453399, y1:0, x2:0.462, y2:1, stop:0.0936281 rgba(245, 245, 245, 255), stop:1 rgba(224, 224, 224, 255));
    border: 1px solid #c3bfbf;
