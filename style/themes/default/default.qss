/* Customize the menu bar. */
QMenuBar {
    color: #414243;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #f7f8f9, stop:0.0680112 #f7f8f9, stop:0.0690112 #f1f3f4, stop:0.514566 #e4e7ea, stop:0.52577 #dde1e5, stop:1 #d0d4da);
    border-top-right-radius: 5px;
    border-top-left-radius: 5px;
    border: 1px solid #7d8389;
    border-bottom: none;
    padding-top: 5px;
    padding-bottom: 0px;
    padding-right: 2px;
    margin: 2px;
    margin-bottom: 0px;
}
QMenuBar::item {
    spacing: 8px;
/* spacing between menu bar items */    padding: 4px 8px;
    background: transparent;
    border-radius: 3px;
}
QMenuBar::item:selected {
/* when selected using mouse or keyboard */    background: qlineargradient(spread:pad, x1:0.453399, y1:0, x2:0.462, y2:1, stop:0 #e0e2e5, stop:1 #bbbec2);
    border: 1px solid #97999d;
}
