/* Customize any plain widget that is a child of a QMainWindow.#ccd3b1 faf3e8*/
QMainWindow > .QWidget {
    background: #d5d9de;
    /*background-image: 
    url(:/pagefold/resources/pagefold/pagefold.png);
    background-position: top right;
    background-repeat: no-repeat;*/    
    padding: 0px;
    border: none;
}

.QWidget , QDialog{
    background: #e8eaed;
    /*font: 12px Calibri , 
    Tahoma, "Times New Roman" ;*/
}

QDialogButtonBox { 
    dialogbuttonbox-buttons-have-icons: 0; 
}

QLabel {
    border: none;
    border-image: none;
    padding: 0;
    background: none;
}

/* Make text in message boxes selectable. */
QMessageBox {
   messagebox-text-interaction-flags: 5; /* LinksAccessibleByMouse | TextSelectableByMouse */ 
}

/* Set the selection colors for all widgets. */
QWidget {
    selection-background-color :#6a7582;
}

/* Make the entire row selected in item views. */
QAbstractItemView {
    show-decoration-selected: 0;
}

QStatusBar {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #f7f8f9, stop:0.0680112 #f7f8f9, stop:0.0690112 #f1f3f4, stop:0.514566 #e4e7ea, stop:0.52577 #dde1e5, stop:1 #d0d4da);
    border-radius: 1px;
    border: 1px solid #7d8389;

}

/* Customize tooltips. */
QToolTip {
    border-width: 3;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #eceef0, stop:1 #d7dbdf);
    border: 1px solid #c9ced3;
    padding: 4px;
    font: normal 11px Calibri, Tohoma;
    opacity: 220;
    border-radius:6px;
}

QListWidget#recentList,QListWidget#recentSessionList {
    background: transparent;
}

/* CSS Window Tab STARTS */
QTabWidget#tabToolWindow::tab-bar:top {
    left: 0px;
}

QTabWidget#tabToolWindow QTabBar::tab:top {
    min-width: 14ex;
    padding: 3px;
    height:20px;
    margin-right: 1px;
    margin-left: 1px;
    border-top-right-radius: 2px;
    border-top-left-radius: 2px;
}

QTabWidget#tabToolWindow QTabBar::tab:top:selected {
    border: 1px solid #8b939c;
    border-bottom:0px solid #ccc;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #fff, stop:1 #eee);
}

QTabWidget#tabToolWindow QTabBar::tab:top:!selected {
    border: 1px solid #8b939c;
}

/* CSS Window Tab ENDS */
QTabWidget::tab-bar:top {
    left: 6px;
}

/*QTabBar:top {
    margin-right:40px; 
    width:100px;
}*/

QTabWidget#tabBaseQTabBar::tab[currentTabText="Start"]:top:selected {
    border-width: 4;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #f9fafa, stop:1 rgba(255, 255, 255, 255));
    border: 1px solid #c3bfbf;
    border-bottom: 0px solid #c3bfbf;;
}

QTabBar::tab:top {
    min-width: 18ex;
    padding: 3px;
    margin-right: 1px;
    margin-left: 1px;
    border-top-right-radius: 5px;
    border-top-left-radius: 5px;
}

QTabBar::tab:top:!selected {
    border-width: 1;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #edeff1, stop:1 #dadee2);
    border: 1px solid #8b939c;
    border-bottom-color: transparent;
    margin-top: 1px;
}

/* make use of negative margins for overlapping tabs */
QTabBar::tab:top:selected {
    border-width: 1;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #f9fafa, stop:1 rgba(255, 255, 255, 255));
    border: 1px solid #c3bfbf;
    border-bottom: 0px solid #c3bfbf;;
}

QTabBar::tab:top:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #fff, stop:1 #eee);
}

QTabBar::tab:top:selected:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #fff, stop:1 #f9f8f8);
}

QTabBar::tab:bottom {
    min-width: 22.1ex;
    padding: 3px;
    margin-bottom:2px;
    margin-right: 0.1px;
    margin-left: 0.1px;
    border-bottom-right-radius: 3px;
    border-bottom-left-radius: 3px;
}

QTabBar::tab:bottom:!selected {
    border-width: 2;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #edeff1, stop:1 #e3e6e9);
    border: 1px solid #8b939c;
    /* same as pane color */
    margin-bottom: 1px;
    /* make non-selected tabs look smaller */
}

/* make use of negative margins for overlapping tabs */
QTabBar::tab:bottom:selected {
/* expand/overlap to the left and right by 4px
    margin-left: -0.5px;
    margin-right: -0.5px;*/    
    border-width: 2;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #efefef, stop:1 #dee0e2);
    border: 1px solid #c3bfbf;
    border-top-color: #efefef;
    /* same as pane color */
}

QTabBar::tab:bottom:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #fbfdfe, stop:1 #edeff1);
}

QTabBar::tab:bottom:selected:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #efefef, stop:1 #dee0e2);
}

QTabBar::tab:first:selected {
    margin-left: 0;

/* the first selected tab has nothing to overlap with on the left */}
QTabBar::tab:last:selected {
    margin-right: 0;

/* the last selected tab has nothing to overlap with on the right */}
QTabBar::tab:only-one {
    margin: 0;

/* if there is only one tab, we don't want overlapping margins */}
QTabBar::close-button {
    image: url(:/tango/images/tango/16x16/actions/process-stop.png);
}

QTabBar::close-button:hover {
    image: url(:/tango/images/tango/16x16/actions/process-stop-over.png);
}

QTabBar::tear {
     image: url(:/tb/images/tree/1.png);
}


QTabBar::scroller { /* the width of the scroll buttons */
     width: 20px; height:5px;
}

