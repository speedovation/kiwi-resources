QMenu {
    color: #414243;
    background-color: #eff1f2;
    margin: 1px;
        padding :2px;
/* some spacing around the menu */    border: none;
}
QMenu::item {
    padding: 4px 25px 4px 20px;
/*color:#7a5924;*/    border: 1px solid transparent;
/* reserve space for selection border */    border-radius: 3px;
}
QMenu::item:selected {
    background: qlineargradient(spread:reflect, x1:0.45241, y1:1, x2:0.441, y2:0.505, stop:0.255526 #e6e9eb, stop:0.960212 #d6d9de);
    border: 1px solid #a0a5ab;
}
QMenu::item:disabled {
    background: transparent;
    color: gray;
}
QMenu::icon:checked {
/* appearance of a 'checked' icon */    background: gray;
    border: 1px inset gray;
    position: absolute;
    top: 1px;
    right: 1px;
    bottom: 1px;
    left: 1px;
}
QMenu::separator {
    height: 2px;
    background-image: url(:/theme/images/theme/default/bar.png);
    background-attachment: fixed;
    background-repeat: repeat-x;
    margin-left: 5px;
    margin-right: 5px;
    margin-top: 2px;
    margin-bottom: 2px;
}
QMenu::indicator {
    width: 13px;
    height: 13px;
}
/* non-exclusive indicator = check box style indicator (see QActionGroup::setExclusive) */
QMenu::indicator:non-exclusive:unchecked {
    image: url(:/images/checkbox_unchecked.png);
}
QMenu::indicator:non-exclusive:unchecked:selected {
    image: url(:/images/checkbox_unchecked_hover.png);
}
QMenu::indicator:non-exclusive:checked {
    image: url(:/theme/images/theme/vista/menu_indicator_disabled.png);
}
QMenu::indicator:non-exclusive:checked:selected {
    image: url(:/theme/images/theme/vista/menu_indicator.png);
}
/* exclusive indicator = radio button style indicator (see QActionGroup::setExclusive) */
QMenu::indicator:exclusive:unchecked {
    image: url(:/images/radiobutton_unchecked.png);
}
QMenu::indicator:exclusive:unchecked:selected {
    image: url(:/images/radiobutton_unchecked_hover.png);
}
QMenu::indicator:exclusive:checked {
    image: url(:/theme/images/theme/vista/radiobutton_on_enabled.png);
}
QMenu::indicator:exclusive:checked:selected {
    image: url(:/theme/images/theme/vista/radiobutton_on_enabled.png);
}
