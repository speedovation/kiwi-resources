/* Customize push buttons and comboboxes. Our read-only combobox
   is very similar to a push button, so they share the same border image. */
QPushButton {
    padding: 2px;
}
QPushButton[flat=false] {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #f7f8f9, stop:0.0680112 #f7f8f9, stop:0.0690112 #f1f3f4, stop:0.514566 #e4e7ea, stop:0.52577 #dde1e5, stop:1 #d0d4da);
    border-radius: 3px;
    border: 1px solid #7d8389;
}
QPushButton[flat=false]:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
        stop:0 #fdfeff,
        stop:0.0680112 #fdfeff,
        stop:0.0690112 #f4f6f7,
        stop:0.514566 #e7e9ed,
        stop:0.52577 #dde2eb,
        stop:1 #dde2eb);
    border-radius: 3px;
    border: 1px solid #939aa2;
}
QPushButton[flat=false]:pressed {
    border-radius: 3px;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
    stop:0 #d0d4da,
    stop:1 #e4e7ea);
    border: 1px solid #7d8389;
}
QPushButton[flat=false]:disabled {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
    stop:0 #e3e6e9,
    stop:0.0680112 #e3e6e9,
    stop:0.0690112 #d1d6db,
    stop:0.514566 #ccd0d6,
    stop:0.52577 #c6cbd2,
    stop:1 #c0c6cd);
    border-radius: 3px;
    border: 1px solid #969ea7;
    color: #979ea8;

}

