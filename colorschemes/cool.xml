<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Default">

  <!-- Common  -->

  <style name="Base" foreground="#f9f9f9" background="#282828" bold="0" italic="0" state="-10"/>
  <style name="BaseHighlight" background="#e0e0e0" />
  <style name="LineHighlight" background="#fefefe" />
  <style name="ChangedText" background="red" />
  <style name="SavedText" background="darkGreen" />
  <style name="NormalText" background="#f9f9f9" />
  <style name="LineNumber" foreground="#bbbbbb" background="#f9f9f9"/>
  <style name="CollapseArea" foreground="#333333" background="#f9f9f9"/>
  <style name="CollapseBlock" foreground="#777777" background="#fbfbfb"/>

  
  <style name="Front" foreground="#00aa00"/>
  <style name="Sidebar" foreground="#00aa00"/>
  
  <style name="Cursor" foreground="#00aa00"/>
  <style name="Marker" foreground="#ffff00"/>

  <style name="BracketMatch" foreground="#00aa00"/>
  <style name="BracketError" foreground="#00aa00"/>
  <style name="FoldIndicator" foreground="#00aa00"/>

  <!-- HTML  -->
  <style name="Doctype" foreground="#628fb5"/>
  <style name="Entity" bold="1"/>
  <style name="Tag" foreground="#FF0033"/>
  <style name="HtmlComment" foreground="#969696"/>
  <style name="Attribute" foreground="#009900"/>
  <style name="HtmlValue" foreground="#de7a39"/>

  <!-- CSS  -->
  <style name="Selector" foreground="#ce395d" bold="0" />
  <style name="Property" foreground="#333333" italic="1" />
  <style name="CssValue" foreground="#2361c5" />
  <style name="CssComment" foreground="#73748e"/>
  <style name="Import" foreground="#395f5f"/>
  <style name="Media" foreground="#00aa00"/>
  <style name="Charset" foreground="#aa55ff"/>
  <style name="CssString" foreground="#0b6125"/>
  <style name="CssSymbol" foreground="#7d7dba"/>
  <style name="CssNumber" foreground="#aa0000"/>
  <style name="CssHighlightBlocks" background="#fdfdfd"/>
  

 <!-- JavaScript  -->
  <style name="Background" foreground="#00aa00"/>
  <style name="NormalJavascript" foreground="#333333"/>
  <style name="JavaScriptComment" foreground="#73748e"/>
  <style name="JavaScriptNumber" foreground="#d35e2c"/>
  <style name="JavaScriptString" foreground="#0b6125"/>
  <style name="JavaScriptOperator" foreground="#808000"/>
  <style name="JavaScriptIdentifier" foreground="#00c500"/>
  <style name="JavaScriptKeyword" foreground="#b83926"/>
  <style name="JavaScriptFuture" foreground="#d35e2c"/>
  <style name="JavaScriptFunction" foreground="blue"/>
  <style name="JavaScriptRegex" foreground="#5500ee"/>

  <!-- PHP  -->
  <style name="PhpBegin" foreground="#E01B1B" bold="1"/>
  <style name="PhpEnd" foreground="#E01B1B" bold="1"/>
  <style name="PhpBackground" foreground="#00aa00"/>
  <style name="PhpNormal" foreground="#282828"/>
  <style name="PhpComment" foreground="#969696"/>
  <style name="PhpNumber" foreground="#ff0000"/>
  <style name="PhpString" foreground="#ce7b00"/>
  <style name="PhpOperator" foreground="#808000"/>
  <style name="PhpIdentifier" foreground="#00c500"/>
  <style name="PhpKeyword" foreground="#0000e6"/>
  <style name="PhpFunction" foreground="#00aaff"/>
  <style name="PhpVariable" foreground="#6d3206"/>
  
   <!-- Python  -->
  <style name="PythonBackground" foreground="#00aa00"/>
  <style name="PythonNormal" foreground="#282828"/>
  <style name="PythonComment" foreground="#969696"/>
  <style name="PythonNumber" foreground="#ff0000"/>
  <style name="PythonString" foreground="#ce7b00"/>
  <style name="PythonOperator" foreground="#808000"/>
  <style name="PythonIdentifier" foreground="#00c500"/>
  <style name="PythonKeyword" foreground="#0000e6"/>
  <style name="PythonFunctions" foreground="#00aaff"/>
  <style name="PythonVariable" foreground="#6d3206"/>


</style-scheme>
